-- GGCMD v2 (Delta Executor -yhteensopiva, tÃ¤ysin mobiili-FE) -- Komennot: ;fly, ;unfly, ;kick, ;fling, ;spam, ;unspam, ;cmds

local Players = game:GetService("Players") local RunService = game:GetService("RunService") local UIS = game:GetService("UserInputService") local LocalPlayer = Players.LocalPlayer local Camera = workspace.CurrentCamera local HttpService = game:GetService("HttpService") local ReplicatedStorage = game:GetService("ReplicatedStorage")

if _G.GGCMD_CONNECTED then return end _G.GGCMD_CONNECTED = true

local flying = false local flyConn local spamConn local spamWord = nil local flingConn

local function getPlayer(name) if not name or name == "me" then return LocalPlayer end for _, p in ipairs(Players:GetPlayers()) do if p.Name:lower():sub(1, #name) == name:lower() then return p end end end

local function toggleFly(state) if state and not flying then flying = true local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait() local hrp = char:WaitForChild("HumanoidRootPart")

local bv = Instance.new("BodyVelocity")
    bv.Name = "GGCMD_Fly"
    bv.MaxForce = Vector3.new(1, 1, 1) * 1e9
    bv.Velocity = Vector3.zero
    bv.Parent = hrp

    flyConn = RunService.RenderStepped:Connect(function()
        if not flying then return end
        local moveVector = UIS:GetGamepadState(Enum.UserInputType.Touch)[1] and UIS:GetGamepadState(Enum.UserInputType.Touch)[1].Position or Vector3.new(0, 0, 0)
        local dir = Vector3.zero

        if moveVector.Magnitude > 0 then
            local camCF = Camera.CFrame
            dir = (camCF.RightVector * moveVector.X + camCF.LookVector * -moveVector.Y)
        end

        if UIS:IsKeyDown(Enum.KeyCode.Space) then dir += Vector3.new(0, 1, 0) end
        if UIS:IsKeyDown(Enum.KeyCode.LeftControl) then dir -= Vector3.new(0, 1, 0) end

        bv.Velocity = dir.Magnitude > 0 and dir.Unit * 70 or Vector3.zero
    end)

elseif not state and flying then
    flying = false
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if hrp and hrp:FindFirstChild("GGCMD_Fly") then hrp.GGCMD_Fly:Destroy() end
    if flyConn then flyConn:Disconnect() end
end

end

local function kickPlayer(name) local target = getPlayer(name) if not target then return end if target.Character then target.Character:Destroy() end if workspace:FindFirstChild(target.Name) then workspace[target.Name]:Destroy() end if Players:FindFirstChild(target.Name) then pcall(function() Players[target.Name]:Destroy() end) end end

local function flingPlayer(name) local target = getPlayer(name) local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") local theirHRP = target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") if not target or not myHRP or not theirHRP then return end

local oldPos = myHRP.Position
flingConn = RunService.Stepped:Connect(function()
    pcall(function()
        myHRP.CFrame = theirHRP.CFrame * CFrame.Angles(0, math.rad(500), 0)
    end)
end)
task.delay(2, function()
    if flingConn then flingConn:Disconnect() end
    myHRP.CFrame = CFrame.new(oldPos + Vector3.new(0, 10, 0))
end)

end

local function startSpam(word) if spamConn then spamConn:Disconnect() end spamWord = word spamConn = RunService.RenderStepped:Connect(function() ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(spamWord, "All") end) end

local function stopSpam() if spamConn then spamConn:Disconnect() end spamConn = nil spamWord = nil end

-- Anti-kick (Client-side) LocalPlayer.Kick = function() return nil end local mt = getrawmetatable(game) setreadonly(mt, false) local oldNamecall = mt.__namecall mt.__namecall = newcclosure(function(self, ...) local args = {...} if getnamecallmethod() == "Kick" and tostring(self) == tostring(LocalPlayer) then return nil end return oldNamecall(self, ...) end)

LocalPlayer.Chatted:Connect(function(msg) local args = msg:split(" ") local cmd = args[1]:lower() local arg = args[2]

if cmd == ";fly" then
    toggleFly(true)
elseif cmd == ";unfly" then
    toggleFly(false)
elseif cmd == ";kick" and arg then
    kickPlayer(arg)
elseif cmd == ";fling" and arg then
    flingPlayer(arg)
elseif cmd == ";spam" and arg then
    startSpam(arg)
elseif cmd == ";unspam" then
    stopSpam()
elseif cmd == ";cmds" then
    ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Komennot: ;fly, ;unfly, ;kick [nimi], ;fling [nimi], ;spam [teksti], ;unspam, ;cmds", "All")
end

end)

print("GGCMD v2 Injected! Kirjoita ;cmds chattiin.")
